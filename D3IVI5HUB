-- Scripts:

local function JAOHWQR_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	local button = script.Parent
	local screenGui = button:FindFirstAncestorOfClass("ScreenGui")
	local TweenService = game:GetService("TweenService")
	
	-- Function to fade out all GUI elements
	local function fadeOutGUI()
		if not screenGui then return end
	
		for _, object in ipairs(screenGui:GetDescendants()) do
			if object:IsA("Frame") or object:IsA("TextLabel") or object:IsA("TextButton") or object:IsA("ImageLabel") or object:IsA("ImageButton") then
				-- Background
				local bgTween = TweenService:Create(object, TweenInfo.new(0.5), {
					BackgroundTransparency = 1
				})
				bgTween:Play()
	
				-- Text
				if object:IsA("TextLabel") or object:IsA("TextButton") then
					local textTween = TweenService:Create(object, TweenInfo.new(0.5), {
						TextTransparency = 1
					})
					textTween:Play()
				end
	
				-- Image
				if object:IsA("ImageLabel") or object:IsA("ImageButton") then
					local imgTween = TweenService:Create(object, TweenInfo.new(0.5), {
						ImageTransparency = 1
					})
					imgTween:Play()
				end
			end
		end
	
		-- Wait for fade to finish, then disable
		task.delay(0.5, function()
			screenGui.Enabled = false
		end)
	end
	
	-- Connect the button
	button.MouseButton1Click:Connect(fadeOutGUI)
	
end
coroutine.wrap(JAOHWQR_fake_script)()
local function EMRWPHG_fake_script() -- Button.LocalScript 
	local script = Instance.new('LocalScript', Button)

	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Fsploit/FeAnimationHub/refs/heads/main/Hub.lua"))()
	end)
	
	
end
coroutine.wrap(EMRWPHG_fake_script)()
