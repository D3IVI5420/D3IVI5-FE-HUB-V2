coroutine.wrap(FCHGW_fake_script)()
local function ICHBNV_fake_script() -- MainBackroundFrame.LocalScript 
	local script = Instance.new('LocalScript', MainBackroundFrame)

	local TweenService = game:GetService("TweenService")
	
	local sideFrame = script.Parent:WaitForChild("SideFrame")
	local scrollsFrame = script.Parent:WaitForChild("ScrollsFrame")
	
	local tabs = {
		Home = "HomeFrame",
		Fe = "FeFrame",
		Flings = "FlingsFrame",
		Admin = "AdminFrame"
	}
	
	local currentVisibleFrame = nil
	local fadeTime = 0.25
	
	-- Tween transparency of all children inside a frame
	local function tweenChildrenTransparency(frame, targetTransparency)
		for _, child in ipairs(frame:GetDescendants()) do
			if child:IsA("TextLabel") or child:IsA("TextButton") or child:IsA("ImageLabel") or child:IsA("ImageButton") then
				local property = child:IsA("TextLabel") or child:IsA("TextButton") and "TextTransparency" or "ImageTransparency"
				if property then
					TweenService:Create(child, TweenInfo.new(fadeTime), {[property] = targetTransparency}):Play()
				end
			end
		end
	end
	
	-- Fades out contents and hides frame
	local function fadeOut(frame)
		tweenChildrenTransparency(frame, 1)
		task.wait(fadeTime)
		frame.Visible = false
	end
	
	-- Fades in contents and shows frame
	local function fadeIn(frame)
		for _, child in ipairs(frame:GetDescendants()) do
			if child:IsA("TextLabel") or child:IsA("TextButton") or child:IsA("ImageLabel") or child:IsA("ImageButton") then
				local property = child:IsA("TextLabel") or child:IsA("TextButton") and "TextTransparency" or "ImageTransparency"
				if property then
					child[property] = 1 -- start invisible
				end
			end
		end
	
		frame.Visible = true
		tweenChildrenTransparency(frame, 0)
	end
	
	-- Switch to selected tab
	local function switchTo(tabName)
		local newFrame = scrollsFrame:FindFirstChild(tabs[tabName])
		if not newFrame then return end
	
		if currentVisibleFrame and currentVisibleFrame ~= newFrame then
			fadeOut(currentVisibleFrame)
		elseif currentVisibleFrame == newFrame then
			return
		end
	
		fadeIn(newFrame)
		currentVisibleFrame = newFrame
	end
	
	-- Connect buttons
	for tabName, frameName in pairs(tabs) do
		local button = sideFrame:FindFirstChild(tabName)
		if button then
			button.MouseButton1Click:Connect(function()
				switchTo(tabName)
			end)
		end
	end
	
	-- Initial setup: hide all, set transparencies
	for _, frameName in pairs(tabs) do
		local frame = scrollsFrame:FindFirstChild(frameName)
		if frame then
			frame.Visible = false
			for _, child in ipairs(frame:GetDescendants()) do
				if child:IsA("TextLabel") or child:IsA("TextButton") or child:IsA("ImageLabel") or child:IsA("ImageButton") then
					local property = child:IsA("TextLabel") or child:IsA("TextButton") and "TextTransparency" or "ImageTransparency"
					if property then
						child[property] = 1
					end
				end
			end
		end
	end
	
	-- Show default tab
	local default = scrollsFrame:FindFirstChild(tabs["Home"])
	if default then
		default.Visible = true
		currentVisibleFrame = default
		tweenChildrenTransparency(default, 0)
	end
	
end
coroutine.wrap(ICHBNV_fake_script)()
local function XHFIGUW_fake_script() -- MainBackroundFrame.LocalScript 
	local script = Instance.new('LocalScript', MainBackroundFrame)

	local frame = script.Parent
	local dragging = false
	local dragInput, mousePos, framePos
	
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			mousePos = input.Position
			framePos = frame.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)
	
	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			local delta = input.Position - mousePos
			frame.Position = UDim2.new(
				framePos.X.Scale, framePos.X.Offset + delta.X,
				framePos.Y.Scale, framePos.Y.Offset + delta.Y
			)
		end
	end)
	
end
coroutine.wrap(XHFIGUW_fake_script)()
